{
    "name": "rsshub-vercel",
    "version": 2,
    "builds": [
        {
            "src": "api/vercel.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["lib/**/*"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/vercel.ts"
        }
    ]
}


